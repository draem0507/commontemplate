<launch4jConfig>
  <dontWrapJar>false</dontWrapJar>
  <headerType>gui</headerType>
  <jar>commontemplate-SNAPSHOT.jar</jar>
  <outfile>CommonTemplate.exe</outfile>
  <errTitle></errTitle>
  <cmdLine></cmdLine>
  <chdir></chdir>
  <priority>normal</priority>
  <downloadUrl>http://java.com/download</downloadUrl>
  <supportUrl></supportUrl>
  <customProcName>true</customProcName>
  <stayAlive>true</stayAlive>
  <manifest></manifest>
  <icon>ctl48.ico</icon>
  <classPath>
    <mainClass>org.commontemplate.tools.viewer.Main</mainClass>
    <cp>%EXEDIR%/*.jar</cp>
    <cp>%PWD%/*.jar</cp>
  </classPath>
  <jre>
    <path></path>
    <minVersion>1.4.2</minVersion>
    <maxVersion></maxVersion>
    <jdkPreference>preferJre</jdkPreference>
    <opt>-Dlaunch4j.exedir=&quot;%EXEDIR%&quot;</opt>
  </jre>
  <splash>
    <file>banner.bmp</file>
    <waitForWindow>true</waitForWindow>
    <timeout>60</timeout>
    <timeoutErr>true</timeoutErr>
  </splash>
</launch4jConfig>